COLUMN TABLE "rfpTexts" 
	("ID" INTEGER CS_INT GENERATED BY DEFAULT AS IDENTITY NOT NULL , 
	"QUESTION" NCLOB COMMENT 'Вопрос', 
	"ANSWER" NCLOB COMMENT 'Ответ', 
	"QA" NCLOB COMMENT 'Вопрос-ответ', 
	"CATEGORY" VARCHAR(30) COMMENT 'Категория вопроса', 
	"AUTHOR" VARCHAR(50) COMMENT 'Автор', 
	PRIMARY KEY ("ID"))